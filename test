import json
import time

def convert_to_netscape(cookies, output_file="cookies.txt", expiry_days=7):
    """
    Mengonversi cookies ke format Netscape dan menyimpannya ke file cookies.txt.
    Semua cookies akan memiliki expiry 7 hari dari sekarang.
    """
    # Waktu saat ini + 7 hari dalam UNIX timestamp
    new_expiry = int(time.time()) + (expiry_days * 24 * 60 * 60)

    # Header format Netscape
    netscape_header = "# Netscape HTTP Cookie File\n# This file was generated by cookie_converter\n\n"
    
    # Format baris cookie Netscape:
    # domain \t flag \t path \t secure \t expiry \t name \t value
    with open(output_file, 'w') as f:
        f.write(netscape_header)
        
        for cookie in cookies:
            domain = cookie.get('domain', '')
            # Flag TRUE/FALSE untuk apakah cookie berlaku untuk semua subdomain
            flag = "TRUE" if domain.startswith('.') else "FALSE"
            path = cookie.get('path', '/')
            # Secure TRUE/FALSE berdasarkan atribut secure
            secure = "TRUE" if cookie.get('secure', False) else "FALSE"
            # Gunakan expiry baru (7 hari dari sekarang), kecuali untuk session cookies
            expiry = 0 if cookie.get('session', False) else new_expiry
            name = cookie.get('name', '')
            value = cookie.get('value', '')
            
            # Tulis baris cookie ke file
            line = f"{domain}\t{flag}\t{path}\t{secure}\t{expiry}\t{name}\t{value}\n"
            f.write(line)

    print(f"Cookies telah dikonversi ke {output_file} dengan expiry 7 hari dari sekarang untuk non-session cookies.")

